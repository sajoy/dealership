<h1>Dealerships </h1>


<form action="/dealership" method="post">
  <label for="name">Dealership's name: </label>
  <input id="name" name="name" type="text">

  <button type="submit">Add Dealership</button>
</form>

<ul>
  <% @dealerships.each() do |dealership| %>
  <li><a href="/dealerships/<%= dealership.id() %>"><%= dealership.name() %></a></li>
  <% end %>
</ul>
